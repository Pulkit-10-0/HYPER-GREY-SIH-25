{
  "expo": {
    "name": "AyuSure E-Tongue Dashboard",
    "slug": "ayusure-ios-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hypergrey.ayusure",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to connect to ESP32 devices for sensor data collection.",
        "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to connect to ESP32 devices for sensor data collection.",
        "NSLocationWhenInUseUsageDescription": "This app needs location access to scan for WiFi networks and Bluetooth devices.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./logo.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.hypergrey.ayusure",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.CHANGE_WIFI_STATE"
      ]
    },
    "web": {
      "favicon": "./logo.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 21
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "24267c4e-07a0-4369-9767-35e222695d33"
      }
    }
  }
}
